    "import pandas as pd\n",
    "\n",
    "df1 = pd.read_csv('../weather.csv')\n",
    "df1['day'] = pd.to_datetime(df1['day'])\n",
    "df2 = pd.read_csv('../weather_data.csv')\n",
    "df2['day'] = pd.to_datetime(df2['day'])\n",
    "\n",
    "merged_data = pd.merge(df2, df1,on = 'day')\n",
    "merged_data"
   ]
